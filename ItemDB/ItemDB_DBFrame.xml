					<OnLeave>
						GameTooltip:Hide();
						ResetCursor();
					</OnLeave>
					<OnClick>
						if ( IsControlKeyDown()  ) then
							DressUpItemLink(ItemDB_DBFrame_ItemButton_GetLink(this:GetParent():GetID() + FauxScrollFrame_GetOffset(ItemDB_DBFrame_ItemList_BrowseScrollFrame)));
						elseif ( IsShiftKeyDown() ) then
							ItemDB_DBFrame_Link2Chat(this:GetParent():GetID() + FauxScrollFrame_GetOffset(ItemDB_DBFrame_ItemList_BrowseScrollFrame));
						elseif ( IsAltKeyDown() ) then
							SetItemRef(ItemDB_DBFrame_ItemButton_GetLink(this:GetParent():GetID() + FauxScrollFrame_GetOffset(ItemDB_DBFrame_ItemList_BrowseScrollFrame)));
						else
							if ( ItemDB_DBFrame_ItemList_ShowOnPlayerCheckButton:GetChecked() ) then
								DressUpItemLink(ItemDB_DBFrame_ItemButton_GetLink(this:GetParent():GetID() + FauxScrollFrame_GetOffset(ItemDB_DBFrame_ItemList_BrowseScrollFrame)));
							end
							<!-- BrowseButton_OnClick(this:GetParent()); -->
						end
					</OnClick>
					<OnUpdate>
						if ( GameTooltip:IsOwned(this) ) then
							ItemDB_DBFrame_ItemButton_OnEnter(this:GetParent():GetID() + FauxScrollFrame_GetOffset(ItemDB_DBFrame_ItemList_BrowseScrollFrame));
